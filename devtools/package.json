    "@vanilla-extract/css": "^1.9.3",
    "ajv": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.7",
    "react-modal": "^3.16.1",
    "uniforms": "^4.0.0-alpha.1",
    "uniforms-bridge-json-schema": "^4.0.0-alpha.1",
    "uniforms-semantic": "^4.0.0-alpha.1",
    "zod": "^3.21.4"
